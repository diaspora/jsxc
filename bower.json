{
  "name": "diaspora_jsxc",
  "version": "0.0.5",
  "homepage": "https://github.com/diaspora/jsxc",
  "description": "XMPP javascript client for diaspora",
  "main": [
    "lib/strophe.js",
    "lib/strophe.disco.js",
    "lib/strophe.caps.js",
    "lib/strophe.muc.js",
    "lib/strophe.vcard.js",
    "lib/jquery-ui.min.css",
    "lib/jquery.mCustomScrollbar.css",
    "lib/jquery.colorbox.css",
    "lib/jquery.colorbox-min.js",
    "lib/jquery.slimscroll.js",
    "lib/jquery.fullscreen.js",
    "build/lib/otr/build/dep/bigint.js",
    "build/lib/otr/build/dep/crypto.js",
    "build/lib/otr/build/dep/eventemitter.js",
    "build/lib/otr/build/dep/salsa20.js",
    "build/lib/otr/lib/const.js",
    "build/lib/otr/lib/helpers.js",
    "build/lib/otr/lib/dsa.js",
    "build/jsxc.css",
    "build/jsxc.webrtc.css",
    "build/jsxc.dep.min.js",
    "build/jsxc.min.js"
  ],
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "xmpp",
    "chat",
    "client",
    "diaspora"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "jsxc.*",
    "build/**/*",
    "example",
    "lib/jquery*.js",
    "lib/strophe.jingle",
    "lib/**/*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~1.11.1",
    "jquery-ui": "~1.10.4",
    "jquery-colorbox": "~1.5.14",
    "jquery.slimscroll": "~1.3.3",
    "jquery-fullscreen": "~1.1.4"
  }
}
